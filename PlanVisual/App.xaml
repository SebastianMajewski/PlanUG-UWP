<Application
    x:Class="PlanVisual.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:PlanVisual.Converters"
    RequestedTheme="Dark">
    <Application.Resources>
        <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <converters:IsNullOrEmptyToVisibilityConverter x:Key="IsNullOrEmptyToVisibilityConverter"/>
        <converters:NotNullToBooleanConverter x:Key="NotNullToBooleanConverter"/>
        <converters:IsSelectedToPropertyStringConverter x:Key="IsSelectedToPropertyStringConverter"/>
    </Application.Resources>
</Application>

